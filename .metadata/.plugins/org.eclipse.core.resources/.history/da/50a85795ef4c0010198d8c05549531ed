package com.jotsamikael.applycam.examCenter;

import com.jotsamikael.applycam.promoter.CreatePromoterRequest;

import jakarta.validation.constraints.NotEmpty;
import jakarta.validation.constraints.NotNull;
import lombok.Builder;
import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
@Builder
public class CreateCenterRequest {
	
	@NotEmpty(message = "Exam center name is mandatory")
    @NotNull(message = "Exam center name is mandatory")
	private String name;
	
	@NotEmpty(message = "Exam center region is mandatory")
    @NotNull(message = "Exam center region is mandatory")
	private String region;
	
	@NotEmpty(message = "Exam center division is mandatory")
    @NotNull(message = "Exam center division is mandatory")
	private String division;

}
